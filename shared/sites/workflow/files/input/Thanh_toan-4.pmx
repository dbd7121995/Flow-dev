        <tri_webbot><![CDATA[function getManager($depId = null, $erpConnectionUid = null) {
	$flag = false;
	$managerId = null;
	$depParent = null;
	while(!$flag) {
		$query = "SELECT * FROM hr_department WHERE id = '" . $depId . "'";
		$rootDep = executeQuery($query, $erpConnectionUid);
		if (!$rootDep) {
			$flag = true;
		}
		
		if ($rootDep[1]['manager_id'] || !$rootDep[1]['parent_id']) {
			$managerId = $rootDep[1]['manager_id'];
			$flag = true;
		}
		$depId = $rootDep[1]['parent_id'];
	}
	
	if ($managerId) {
		$query = "SELECT ru.login AS username , rp.email AS email
				  FROM res_users ru
				  INNER JOIN res_partner rp ON ru.partner_id = rp.id  WHERE ru.id = '" . $managerId . "'";
		$manager = executeQuery($query, $erpConnectionUid);
		if ($manager) {
			return $manager[1];
		}
	}
	
	return false;
}

// Dựa vào line manager chọn lúc tạo yêu cầu lấy ra được department_uid và từ đó tính ra root department
$erpConnectionUid = '6215105755981548258fc39075421333';
// Mặc định sét cứng tài khoản trucphuong.nguyen
@@head = '67398892158f5c8bf86c345097876548';
$query = "SELECT * FROM hr_department WHERE name = '". @@department_label ."'";
$dep = executeQuery($query, $erpConnectionUid);

if ($dep) {
	$manager = getManager($dep[1]['id'], $erpConnectionUid);

	if ($manager) {
		$query = "SELECT * FROM USERS WHERE USR_EMAIL = '". $manager['email'] ."' OR USR_USERNAME = '". $manager['username'] ."'";
		$userFlow = executeQuery($query);
		if ($userFlow) {
			@@head = $userFlow[1]['USR_UID'];
			@@head_email = $userFlow[1]['USR_EMAIL'];
		}
	} 
}

// Set Email line manager
