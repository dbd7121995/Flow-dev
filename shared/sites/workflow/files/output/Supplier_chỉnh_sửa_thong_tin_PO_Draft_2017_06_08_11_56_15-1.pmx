]]></tri_webbot>
        <tri_param></tri_param>
        <tri_title>Case Link</tri_title>
        <tri_description><![CDATA[Link để đăng nhập tự động vào process maker]]></tri_description>
      </record>
      <record>
        <tri_uid>5777455715937d26baa2b59075001030</tri_uid>
        <pro_uid>260912018592d3b1f6eed06058509693</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$categoryId = @@po_tiki_category_id;
$partnerId = @@po_tiki_partner_id;
$po_name = @@po_name;
$currentUser = @@USER_LOGGED;
$newCaseId = @@APPLICATION;
$link = PMFCaseLink(@@APPLICATION);
@@app_link = $link;

$data = executeQuery("
SELECT IF(COUNT(1) = 1, U.USR_UID, DEPA.DEP_MANAGER) USR_UID
FROM PMT_TIKI_SUPPLIER MAP
INNER JOIN DEPARTMENT DEPA
ON DEPA.DEP_UID = MAP.DEP_UID

INNER JOIN GROUP_USER GU
ON GU.GRP_UID = MAP.GRP_UID

LEFT JOIN USERS U
ON U.USR_UID = GU.USR_UID
AND U.DEP_UID = MAP.DEP_UID

WHERE MAP.TIKI_PARTNER_ID = '" . $partnerId . "'
AND MAP.TIKI_CATEGORY_ID='" . $categoryId . "'
GROUP BY MAP.TIKI_PARTNER_ID, MAP.TIKI_CATEGORY_ID");

if ($newCaseId) {
    $c = new Cases();
    $aCase = $c->loadCase($newCaseId);
    $aCase['APP_STATUS'] = 'TO_DO';
    $c->updateCase($newCaseId, $aCase);

    $designatedUser = $data[1]['USR_UID'];

    $query = "SELECT DEL_INDEX FROM APP_DELEGATION WHERE APP_UID = '{$newCaseId}' AND DEL_THREAD_STATUS='OPEN'";
    $case = executeQuery($query);

    $c2 = new Cases();
    $c2->reassignCase($newCaseId, $case[1]['DEL_INDEX'], $currentUser, $designatedUser);

    $from = new Users();
    $from = $from->Load(@@USER_LOGGED);
    $to = new Users();
    $to = $to->Load($designatedUser);
	
	$emailData = $aCase;
	$emailData['app_link'] = $link;
    PMFSendMessage($newCaseId, $from['USR_EMAIL'], $to['USR_EMAIL'], '', '',
      "{$to['USR_FIRSTNAME']} {$to['USR_LASTNAME']} ơi, bạn nhận được đơn đặt hàng {$aCase['PO_NAME']} từ Tiki", 'incomming_po.html', $emailData, $po_name);
}]]></tri_webbot>
