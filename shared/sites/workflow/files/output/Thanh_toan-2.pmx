        <tri_webbot><![CDATA[function getManager($depId = null, $erpConnectionUid = null) {
	$flag = false;
	$managerId = null;
	$depParent = null;
	while(!$flag) {
		$query = "SELECT * FROM hr_department WHERE id = '" . $depId . "'";
		$rootDep = executeQuery($query, $erpConnectionUid);
		if (!$rootDep) {
			$flag = true;
		}
		
		if ($rootDep[1]['manager_id'] || !$rootDep[1]['parent_id']) {
			$managerId = $rootDep[1]['manager_id'];
			$flag = true;
		}
		$depId = $rootDep[1]['parent_id'];
	}
	
	if ($managerId) {
		$query = "SELECT ru.login AS username , rp.email AS email
				  FROM res_users ru
				  INNER JOIN res_partner rp ON ru.partner_id = rp.id  WHERE ru.id = '" . $managerId . "'";
		$manager = executeQuery($query, $erpConnectionUid);
		if ($manager) {
			return $manager[1];
		}
	}
	
	return false;
}

