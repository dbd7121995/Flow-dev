        <tri_webbot><![CDATA[function getUserByDepartment($departmentUid = null) {
	$users = array();
	
	$query = "SELECT * FROM USERS WHERE DEP_UID = '$departmentUid'";
	$result = executeQuery($query);

	foreach($result as $_result) {
		$users[] = $_result['USR_UID'];
	}

	if ($users) {
		return $users;
	}
	
	
	return false;
}

$saleDepartmentUid = '8235936345992e36f41c509021849814';
$fnaDepartmentUid = '2347755895992e3060f38d8004586928';
$legalDepartmentUid = '7568526845992e319b1b3c3076247009';
$en30DepartmentUid = '3587243315997ca4477a850008885852';
$defaultDepartment = [$saleDepartmentUid, $fnaDepartmentUid, $legalDepartmentUid, $en30DepartmentUid];

$defaultEn30User = array();
@@saleUser = array();
@@fnaUser = array();
@@legalUser = array();

foreach($defaultDepartment as $_defaultDepartment) {
	$query = "SELECT * FROM DEPARTMENT WHERE DEP_UID = '$_defaultDepartment'";
	$result = executeQuery($query);
	$currentUser = null;

	if ($result[1] && is_array($result[1])) {
		$query = "SELECT * FROM USERS WHERE DEP_UID = '".$result[1]['DEP_UID']."'";
		$result = executeQuery($query);

		foreach($result as $_result) {
			switch ($_defaultDepartment) {
				case $saleDepartmentUid:
				@@saleUser[] = $_result['USR_UID'];
				break;
				case $fnaDepartmentUid:
				@@fnaUser[] = $_result['USR_UID'];
				break;
				case $legalDepartmentUid:
				@@legalUser[] = $_result['USR_UID'];
				break;
				case $en30DepartmentUid:
				$defaultEn30User[] = $_result['USR_UID'];
				break;
			}
		}

		$isStop = 0;

		$parentDepUid = $result[1]['DEP_UID'];
		while (!$isStop) {
			$query = "SELECT * FROM DEPARTMENT WHERE DEP_PARENT = '$parentDepUid'";
			$result = executeQuery($query);

			if ($result[1] && is_array($result[1])) {
				$parentDepUid = $result[1]['DEP_UID'];
				$users = getUserByDepartment($parentDepUid);
				if ($users) {
					foreach($users as $user) {
							switch ($_defaultDepartment) {
								case $saleDepartmentUid:
									@@saleUser[] = $user;
									break;
								case $fnaDepartmentUid:
									@@fnaUser[] = $user;
									break;
								case $legalDepartmentUid:
									@@legalUser[] = $user;
									break;
								case $en30DepartmentUid:
									$defaultEn30User[] = $user;
									break;
							}
					}
				}
			} else {
				$isStop = 1;
			}
		}
	}
}

// If null fna_department Or legal_department => default EN30_department
if (!@@fnaUser) {
	@@fnaUser = $defaultEn30User;
}

if (!@@legalUser) {
	@@fnaUser = $defaultEn30User;
}

echo '<pre>';
echo var_dump(@@saleUser);
echo var_dump(@@fnaUser);
echo var_dump(@@legalUser);
echo '</pre>';]]></tri_webbot>
